		"buffer-image-size": "^0.6.4",
		"cors": "^2.8.5",
		"crypto": "^1.0.1",
		"dotenv": "^4.0.0",
		"express": "^4.18.2",
		"express-session": "^1.17.3",
		"fetch": "^1.1.0",
		"firebase-admin": "^12.0.0",
		"get-stream": "^8.0.1",

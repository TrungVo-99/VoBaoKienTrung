{
	"name": "realkorean-api",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "npm run serve:gcloud",
		"sequelize": "./node_modules/.bin/sequelize",
		"sequelize:migrate": "./node_modules/.bin/sequelize db:migrate",
		"build": "npm run clear && npm run build-ts && npm run build-babel",
		"serve": "nodemon dist/server.js",
		"watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-babel\" \"npm run serve\"",
		"test": "jest --forceExit",
		"build-ts": "npx tsc",
		"watch-ts": "tsc -w",
		"build-webpack": "webpack",
		"watch-webpack": "webpack --watch",
		"tslint": "tslint -c tslint.json -p tsconfig.json",
		"tslint-fix": "tslint -c tslint.json -p tsconfig.json --fix",
		"copy-static-assets": "node copyStaticAssets.js",
		"debug": "npm run build && npm run watch-debug",
		"serve-debug": "nodemon --inspect dist/server.js",
		"watch-debug": "npm run clear && concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-babel\"",
		"logs": "heroku logs --tail",
		"dev": "node dist/index.js",
		"start-dev": "node dist/index.js",
		"build-babel": "npx babel build --out-dir dist --source-maps",
		"watch-babel": "./node_modules/.bin/babel-watch build/index.js",
		"watch-babel:prod": "NODE_ENV=production ./node_modules/.bin/babel-watch build/index.js",
		"watch-babel:prod:win": ".\\node_modules\\.bin\\babel-watch build\\index.js",
		"docker:pg": "cd docker && docker-compose -f docker-compose.postgresql.yml up -d",
		"docker:pg:stop": "cd docker && docker-compose -f docker-compose.postgresql.yml stop",
		"docker:app:build": "docker build -t demo-app .",
		"docker:app:run": "docker run -dit -p 3000:3000 demo-app",
		"clear": "rm -rf ./build && rm -rf ./dist",
		"heroku:config:push": "heroku config:push -c -f .env -o",
		"w1": "npx tsc -w",
		"w2": "npx babel-watch build/index.js",
		"start:dev": "concurrently -k -p \"[{name}]\" -n \"WATCH,HTTP,OTHER\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run w1\" \"npm run w2\"",
		"migrate": "sequelize db:migrate --env development",
		"prepare": "cd ../../ && husky install src/problem5/.husky",
		"lint": "npx eslint --ignore-path .eslintignore \"src/**/*.ts\" --fix",
		"lint:check": "eslint --ignore-path .eslintignore \"src/**/*.ts\"",
		"db:migrate": "npx sequelize-cli db:migrate --name",
		"db:migration": "npx sequelize migration:create --name",
		"db:migrate:undo": "npx sequelize db:migrate:undo --name",
		"format": "prettier --write .",
		"postinstall": "cd ../../ && npx husky install src/problem5/.husky"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown",
		"*.ts": [
			"npm run lint",
			"git add ."
		]
	},
	"jest": {
		"globals": {
			"__TS_CONFIG__": "tsconfig.json"
		},
		"moduleFileExtensions": [
			"ts",
			"js"
		],
		"transform": {
			"^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
		},
		"testMatch": [
			"**/test/**/*.test.(ts|js)"
		],
		"testEnvironment": "node",
		"moduleNameMapper": {
			"@/(.*)": "./src/$1"
		},
		"moduleDirectories": [
			"node_modules",
			"src"
		]
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.23.4",
		"@babel/core": "^7.23.7",
		"@babel/node": "^7.22.19",
		"@babel/plugin-syntax-import-source": "^7.23.7",
		"@babel/plugin-transform-runtime": "^7.23.7",
		"@babel/preset-env": "^7.23.8",
		"@babel/preset-typescript": "^7.23.3",
		"@commitlint/cli": "^18.4.4",
		"@types/async": "^3.2.24",
		"@types/bluebird": "^3.5.42",
		"@types/body-parser": "^1.19.5",
		"@types/bson": "^4.2.0",
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.17",
		"@types/del": "^4.0.0",
		"@types/dotenv": "^2.0.20",
		"@types/errorhandler": "^1.5.3",
		"@types/express": "^4.17.21",
		"@types/express-serve-static-core": "^4.17.41",
		"@types/express-session": "^1.17.10",
		"@types/jest": "^29.5.11",
		"@types/jwt-simple": "^0.5.36",
		"@types/lodash": "^4.14.202",
		"@types/md5": "^2.3.5",
		"@types/morgan": "^1.9.9",
		"@types/multer": "^1.4.11",
		"@types/node": "^20.10.7",
		"@types/sequelize": "^4.28.19",
		"@types/socket.io": "^3.0.2",
		"@types/swagger-ui-express": "^4.1.6",
		"babel-loader": "^9.1.3",
		"babel-plugin-istanbul": "^6.1.1",
		"babel-plugin-root-import": "^6.6.0",
		"babel-polyfill": "^6.26.0",
		"babel-register": "^6.26.0",
		"babel-watch": "^7.8.1",
		"concurrently": "^8.2.2",
		"eslint": "^8.56.0",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.0",
		"nodemon": "^3.0.2",
		"sequelize-cli": "^6.6.2",
		"ts-jest": "^29.1.1",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"tslint": "^6.1.3",
		"typescript": "^5.3.3",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"@babel/runtime": "^7.23.8",
		"@commitlint/config-conventional": "^18.4.4",
		"@sendgrid/mail": "^8.1.0",
		"@socket.io/redis-emitter": "^5.1.0",
		"@types/bcrypt": "^5.0.2",
		"@types/gifsicle": "^5.2.2",
		"@typescript-eslint/eslint-plugin": "^6.18.1",
